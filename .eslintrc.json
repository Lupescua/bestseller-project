{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "standard",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "vue/multi-word-component-names": "off"
  }
}
